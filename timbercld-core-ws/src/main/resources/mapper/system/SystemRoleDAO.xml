<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.timbercld.ws.system.dao.SystemRoleDAO">

    <insert id="insertSubSystemRole" parameterType="com.timbercld.ws.system.entity.SystemRoleEntity">
        insert into system_role (id, name,  creator, create_date,comment) values ( #{id}, #{name},  #{creator}, now(), #{comment})
    </insert>
</mapper>